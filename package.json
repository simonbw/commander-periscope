{
  "name": "commander-periscope",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "repository": "https://github.com/simonbw/commander-periscope",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "clarify": "^2.1.0",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.28.7",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "glob": "^7.1.2",
    "hard-source-webpack-plugin": "^0.5.14",
    "immutable": "^4.0.0-rc.9",
    "js-cookie": "^2.2.0",
    "morgan": "^1.9.0",
    "nyc": "^11.4.1",
    "pretty-error": "^2.1.1",
    "prop-types": "^15.6.0",
    "pubsub-js": "^1.5.7",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-json-pretty": "^1.7.4",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-multi": "^0.1.12",
    "redux-thunk": "^2.2.0",
    "serve-favicon": "^2.4.5",
    "shortid": "^2.2.8",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "style-loader": "^0.19.1",
    "trace": "^3.1.0",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.3",
    "@storybook/addon-links": "^3.3.3",
    "@storybook/react": "^3.3.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-immutable": "^2.0.0-alpha.1",
    "mocha": "^4.0.1",
    "nodemon": "^1.13.3",
    "puppeteer": "^0.13.0"
  },
  "nyc": {
    "all": true,
    "cache": true,
    "exclude": [
      "dist/**",
      "coverage/**",
      "stories/**"
    ],
    "reporter": "html"
  },
  "scripts": {
    "start": "NODE_ENV=dev DEBUG=commander-periscope:* nodemon --inspect $NODE_DEBUG_OPTION src/server/index.js --exec babel-node",
    "build": "$npm_execpath run build-server; $npm_execpath run build-client",
    "build-client": "webpack",
    "build-server": "babel src/server/ -d dist/server; babel src/common/ -d dist/common",
    "start-prod": "NODE_ENV=prod node dist/server/index.js",
    "test": "NODE_ENV=test nyc mocha --exit --require babel-register --recursive test",
    "test-unit": "NODE_ENV=test mocha --exit --require babel-register --recursive test/server test/client",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  }
}
